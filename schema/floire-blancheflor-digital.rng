                  <ref name="tei_model.global"/>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <ref name="tei_item"/>
                  <zeroOrMore>
                     <ref name="tei_model.global"/>
                  </zeroOrMore>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <ref name="tei_label"/>
